		<application name="publisherpython" stream="yes" multiple="no"  restart="no" starting_time="0" retries="0" stopping_time="0" pass_info="yes" log_directory="default" >
		  <start executable="test/publisher.py" args="" />
		</application>


	</applications>

</config>
