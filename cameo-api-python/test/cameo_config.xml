		<application name="pybind" stream="yes" multiple="no"  restart="no" starting_time="0" retries="0" stopping_time="0" pass_info="yes"  >
		  <start executable="/usr/local/src/cameo-api-cpp/cameo-python/test/mytest_test.py" args="" />
